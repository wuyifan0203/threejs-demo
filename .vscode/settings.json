{
  "liveServer.settings.port": 5500,
  "editor.fontSize": 14,
  "files.autoSave": "onFocusChange",
  "fileheader.customMade": {
    "Author": "git config user.name && git config user.email",
    "Date": "Do not edit", // 文件创建时间(不变)
    "LastEditors": "git config user.name && git config user.email", // 文件最后编辑者
    "LastEditTime": "Do not edit", // 文件最后编辑时间
    "FilePath": "Do not edit", // 文件在项目中的相对路径 自动更新
    "custom_string_obkoro1_copyright": "Copyright (c) 2024 by wuyifan email: 1208097313@qq.com, All Rights Reserved."
  },

  "fileheader.configObj": {
    "prohibitAutoAdd": ["json", "md"], // 禁止.json .md文件，自动添加头部注释
    "folderBlacklist": ["node_modules", "dist", "build"]
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.fixAll.stylelint": "explicit"
  },
  "cSpell.words": [
    "broadphase",
    "commitlint",
    "contactmaterials",
    "CONVEXPOLYHEDRON",
    "FXAA",
    "GLTF",
    "HEIGHTFIELD",
    "iconfont",
    "importmap",
    "Octree",
    "Phong",
    "physijs",
    "pixelmatch",
    "plusplus",
    "raycaster",
    "slerp",
    "threejs",
    "TRIMESH",
    "unproject",
    "vitepress",
    "WBOIT",
    "wireframe",
    "XYZE",
    "Yifan"
  ]
}
